{
  "image": "sbrunner/scan-to-paperless:latest",
  "name": "sbrunner-scan-to-paperless",
  "volume_bindings": [
    {
      "host_volume_file": "/paperless/scan",
      "mount_point": "/source",
      "type": "rw"
    },
    {
      "host_volume_file": "/paperless/consume",
      "mount_point": "/destination",
      "type": "rw"
    }
  ]
}
